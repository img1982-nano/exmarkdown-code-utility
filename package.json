{
  "name": "exmarkdown-code-utility",
  "version": "2.3.1",
  "type": "module",
  "description": "‚ùè Code utility plugin for svelte-exmarkdown",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/exmarkdown-code-utility/issues",
  "homepage": "https://github.com/jill64/exmarkdown-code-utility#readme",
  "files": [
    "dist",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    "./styles": {
      "types": "./dist/styles.d.ts",
      "svelte": "./dist/styles.js",
      "default": "./dist/styles.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "code",
    "markdown",
    "plugin",
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/exmarkdown-code-utility.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "pnpm build && vite preview",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "test": "playwright test",
    "prepack": "npm run build",
    "check": "svelte-kit sync && npx svelte-check",
    "format": "npx prettier --write . --plugin prettier-plugin-svelte .",
    "lint": "npm run check && npx eslint ."
  },
  "peerDependencies": {
    "svelte": "^4.0.0",
    "svelte-exmarkdown": "^3.0.0"
  },
  "devDependencies": {
    "@jill64/sentry-sveltekit-cloudflare": "1.3.3",
    "@jill64/svelte-dark-theme": "2.2.3",
    "@jill64/svelte-input": "1.5.2",
    "@jill64/svelte-toast": "1.2.0",
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-cloudflare": "2.3.3",
    "@sveltejs/kit": "1.27.6",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-svelte": "2.35.1",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.7",
    "svelte-exmarkdown": "3.0.1",
    "svelte-qparam": "0.1.0",
    "typescript": "5.3.2",
    "unified": "11.0.4",
    "vite": "5.0.4"
  },
  "dependencies": {
    "@jill64/async-observer": "1.2.0",
    "svelte-feather-icons": "4.0.1",
    "svelte-highlight": "7.4.2",
    "unist-util-visit": "5.0.0"
  }
}
